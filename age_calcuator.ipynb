   "source": [
    "\"\"\"\n",
    "Calculating age based on datetime library\n",
    "\"\"\"\n",
    "import datetime\n",
    "\n",
    "bird = datetime.datetime(1997, 6, 23, 10, 30)\n",
    "now = datetime.datetime.now()\n",
    "\n",
    "sen = now - bird\n",
    "print(f\"My age is: {sen.days / 365.25:.2f} years.\")"
   ]
  }
