    "import re\n",
    "\n",
    "email = input('Enter your e-mail address: ')\n",
    "valid_mail = r\"(\\w+)@(\\w+).(\\w{2,3})\"\n",
    "\n",
    "if re.search(valid_mail, email) == None:\n",
    "    print(\"Not OK!\") \n",
    "else:\n",
    "    print('OK') "
   ]
